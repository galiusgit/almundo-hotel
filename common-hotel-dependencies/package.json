{
  "name": "common-hotel-dependencies",
  "version": "1.0.0",
  "description": "Almundo common hotel dependencies",
  "license": "MIT",
  "email": "jorgeromen27@gmail.com",
  "author": {
    "email": "jorgeromen27@gmail.com",
    "name": "Jorge Romero",
    "url": "https://www.linkedin.com/in/jorge-luis-romero-casta%C3%B1eda-04791167/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/galiusgit/almundo-hotel.git"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsc --p tsconfig.json",
    "deploy:": "node dist/main.js",
    "deploy:dev": "nodemon",
    "deploy:debug": "nodemon --config nodemon-debug.json",
    "predeploy:prod": "rimraf dist && tsc",
    "deploy:prod": "node dist/main.js",
    "deploy:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "gulp": "^3.9.1",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-tslint": "^8.1.3",
    "gulp-typedoc": "^2.2.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.0.4",
    "mocha-typescript": "^1.1.12",
    "typescript": "^2.8.0",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^6.0.0",
    "tsconfig-paths": "^3.3.1",
    "tslint": "5.3.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
