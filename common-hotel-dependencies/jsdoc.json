{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./dist/js-docs",
    "encoding": "utf8",
    "recurse": true,
    "package": "package.json",
    "tutorials": "node_modules/tui-jsdoc-template/demo/samples",
    "template": "node_modules/tui-jsdoc-template"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "templates": {
    "name": "Tui JSDoc Template",
    "footerText": "My awesome footer text",
    "useCollapsibles": true,
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true
    },
    "path": "ink-docstrap",
    "theme": "cerulean",
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a",
    "tabNames": {
      "api": "API",
      "tutorials": "Examples"
    }
  }
}