{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": false,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es6",
    "sourceMap": false,
    "allowJs": true,


    "noUnusedLocals": true,
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "exclude": [
    "dist",
    "node_modules",
    "**/*.spec.ts",
    "src"
  ],
  "files": [
    "gulpfile.ts"
  ]
}