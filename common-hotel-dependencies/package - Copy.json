{
  "name": "nest-typescript-starter",
  "version": "0.0.1",
  "title": "Nest typescript starter",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "author": "Jorge Romero <jorgeromen27@gmail.com>",
  "email": "jorgeromen27@gmail.com",
  "app": {
    "dist": "./dist",
    "host": "localhost:3100",
    "port": "3100",
    "termsOfService": "This is free",
    "licenseUrl": "https://en.wikipedia.org/wiki/MIT_License",
    "tsAppProgramFiles": "./src/modules/**/*.ts",
    "externalDocs": {
      "description": "My external docs",
      "url": "https: //www.azlyrics.com/lyrics/scorpions/maybeimaybeyou.html"
    },
    "swaggerDocs": {
      "description": "Swagger docs",
      "url": "localhost:3100/docs"
    }
  },
  "scripts": {
    "compile": "npm run compile:server && npm run compile:client",
    "compile:server": "tsc --p tsconfig.json",
    "compile:client": "tsc --p tsconfig.client.json",
    "start": "node index.js",
    "start:watch": "nodemon",
    "start:prod": "node dist/ServerApp.js",
    "prestart:prod": "tsc --p tsconfig.json",
    "clean": "rimraf dist",
    "copy-swagger-ui": "ncp ./src/swagger-ui ./dist/swagger-ui",
    "copy-package": "ncp ./package.json ./dist/package.json",
    "package-schema": "typescript-json-schema ./src/package.d.ts Package --strictNullChecks true --defaultProps true --required true --out ./dist/package.schema.json",
    "build": "npm run clean && npm run compile && npm run copy-package && npm run package-schema && npm run copy-swagger-ui && npm run generators",
    "build:docs": "gulp typedoc && gulp jsdoc",
    "deploy": "node dist/ServerApp.js",
    "stop": "node node_modules/pkill --signal SIGINT ServerApp",
    "generators": "node dist/AppGenerators.js",
    "build-and-test": "npm run compile:server && npm run copy-package && npm run package-schema && mocha --reporter spec dist/test/**/*.js",
    "test": "mocha --reporter spec dist/test/**/*.js"
  },
  "dependencies": {
    "@nestjs/common": "^4.6.6",
    "@nestjs/core": "^4.6.6",
    "@nestjs/microservices": "^4.6.6",
    "@nestjs/mongoose": "3.0.1",
    "@nestjs/swagger": "^1.1.4",
    "@nestjs/testing": "^4.6.6",
    "@nestjs/websockets": "^4.6.6",
    "@types/json-schema": "^6.0.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "lodash": "^4.17.5",
    "method-override": "^2.3.10",
    "pkill": "^2.0.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.7"
  },
  "devDependencies": {
    "@types/assert": "0.0.31",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.1",
    "@types/glob": "^5.0.35",
    "@types/gulp": "^3.8.35",
    "@types/method-override": "0.0.31",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.7",
    "@types/rimraf": "^2.0.2",
    "@types/undertaker": "^1.2.0",
    "assert": "^1.4.1",
    "glob": "^7.1.2",
    "glob-to-regexp": "^0.4.0",
    "gulp": "^3.9.1",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-tslint": "^8.1.3",
    "gulp-typedoc": "^2.2.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.0.4",
    "mocha-typescript": "^1.1.12",
    "ncp": "^2.0.0",
    "nodemon": "^1.17.2",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tui-jsdoc-template": "^1.2.2",
    "typedoc": "^0.9.0",
    "typedoc-plugin-markdown": "^1.0.14",
    "typescript": "^2.7.2",
    "typescript-json-schema": "^0.21.0"
  }
}
